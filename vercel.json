{
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "functions": {
        "api/main.py": {
            "memory": 512,
            "maxDuration": 5
        }
    }
}